{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "add-task-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/addTask/AddTaskWebPart.js",
          "manifest": "./src/webparts/addTask/AddTaskWebPart.manifest.json"
        }
      ]
    },
    "site-grid-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/siteGridView/SiteGridViewWebPart.js",
          "manifest": "./src/webparts/siteGridView/SiteGridViewWebPart.manifest.json"
        }
      ]
    },
    "add-site-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/addSiteList/AddSiteListWebPart.js",
          "manifest": "./src/webparts/addSiteList/AddSiteListWebPart.manifest.json"
        }
      ]
    },
    "add-job-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/addJobDetails/AddJobDetailsWebPart.js",
          "manifest": "./src/webparts/addJobDetails/AddJobDetailsWebPart.manifest.json"
        }
      ]
    },
    "add-task-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/addTaskList/AddTaskListWebPart.js",
          "manifest": "./src/webparts/addTaskList/AddTaskListWebPart.manifest.json"
        }
      ]
    },
    "jobdetailsgrid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/jobdetailsgrid/JobdetailsgridWebPart.js",
          "manifest": "./src/webparts/jobdetailsgrid/JobdetailsgridWebPart.manifest.json"
        }
      ]
    },
    "configuration-grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/configurationGrid/ConfigurationGridWebPart.js",
          "manifest": "./src/webparts/configurationGrid/ConfigurationGridWebPart.manifest.json"
        }
      ]
    },
    "edit-task-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/editTaskList/EditTaskListWebPart.js",
          "manifest": "./src/webparts/editTaskList/EditTaskListWebPart.manifest.json"
        }
      ]
    },
    "view-job-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/viewJobDetails/ViewJobDetailsWebPart.js",
          "manifest": "./src/webparts/viewJobDetails/ViewJobDetailsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js",
      "globalName": "jquery"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
      "globalName": "bootstrap",
      "globalDependencies": [
        "jquery"
      ]
    },
    "sp-init": {
      "path": "/_layouts/15/init.js",
      "globalName": "$_global_init"
    },
    "microsoft-ajax": {
      "path": "/_layouts/15/MicrosoftAjax.js",
      "globalName": "Sys",
      "globalDependencies": [
        "sp-init"
      ]
    },
    "sp-runtime": {
      "path": "/_layouts/15/SP.Runtime.js",
      "globalName": "SP",
      "globalDependencies": [
        "microsoft-ajax"
      ]
    },
    "sharepoint": {
      "path": "/_layouts/15/SP.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    }
  },
  "localizedResources": {
    "AddTaskWebPartStrings": "lib/webparts/addTask/loc/{locale}.js",
    "SiteGridViewWebPartStrings": "lib/webparts/siteGridView/loc/{locale}.js",
    "AddSiteListWebPartStrings": "lib/webparts/addSiteList/loc/{locale}.js",
    "AddJobDetailsWebPartStrings": "lib/webparts/addJobDetails/loc/{locale}.js",
    "AddTaskListWebPartStrings": "lib/webparts/addTaskList/loc/{locale}.js",
    "JobdetailsgridWebPartStrings": "lib/webparts/jobdetailsgrid/loc/{locale}.js",
    "ConfigurationGridWebPartStrings": "lib/webparts/configurationGrid/loc/{locale}.js",
    "EditTaskListWebPartStrings": "lib/webparts/editTaskList/loc/{locale}.js",
    "ViewJobDetailsWebPartStrings": "lib/webparts/viewJobDetails/loc/{locale}.js"
  }
}
